+                    view.context.getString(R.string.wallpaper_color_title)
+                return
+            }
+        }
+    }
+
+    override fun getLayoutResId(): Int {
+        return R.layout.color_seed_option
+    }
+
+    class PreviewInfo(var lightColors: IntArray, var darkColors: IntArray)
